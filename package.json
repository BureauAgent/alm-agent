{
  "name": "ai-bot-project",
  "version": "1.0.0",
  "description": "AI-powered bot with Twitter and Web integration. Supports local models (Ollama) and OpenAI.",
  "main": "dist/bot/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/bot/index.js",
    "dev": "ts-node src/bot/index.ts",
    "dev:web": "BOT_MODE=web ts-node src/bot/index.ts",
    "dev:twitter": "BOT_MODE=twitter ts-node src/bot/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "twitter-api-v2": "^1.15.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@solana/web3.js": "^1.87.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  },
  "keywords": [
    "bot",
    "twitter",
    "web",
    "ai",
    "neural network",
    "ollama",
    "openai",
    "chatbot"
  ],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}